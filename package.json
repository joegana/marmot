{
  "name": "marmot-web",
  "version": "1.0.0",
  "description": "Release management suite with continuous delivery support for deployments",
  "private": true,
  "dependencies": {
    "egg": "^2.2.1",
    "egg-scripts": "^2.5.0",
    "egg-sequelize": "^3.1.4",
    "marmot-dingtalk": "^1.0.4",
    "marmot-view": "^1.0.0",
    "mysql2": "^1.5.3"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "egg-bin": "^4.3.5",
    "egg-mock": "^3.14.0",
    "eslint": "^4.11.0",
    "eslint-config-egg": "^6.0.0",
    "git-contributor": "^1.0.8",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "start": "eggctl start --port=9900 --title=egg-server-marmot-web",
    "stop": "eggctl stop --title=egg-server-marmot-web",
    "dev": "egg-bin dev -p 9900",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint . --fix",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "contributor": "git-contributor"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/macacajs/marmot.git"
  },
  "license": "MIT"
}
